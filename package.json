    "file-type": "^20.4.0",
    "misc-utils-of-mine-generic": "^0.2.34"
  },
  "devDependencies": {
    "@types/node": "^22.13.5",
    "ava": "^6.2.0",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "puppeteer": "^1.20.0"
  },
  "keywords": [
    "Node.js",
    "camera",
    "webcam",
    "video",
    "capture",
    "record",
